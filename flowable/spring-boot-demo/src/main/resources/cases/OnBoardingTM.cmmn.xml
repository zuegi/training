  <case id="onBoardingTM" name="OnBoardingTM" flowable:initiatorVariableName="initiator" flowable:candidateStarterGroups="flowableUser">
    <documentation>Der Ort an dem der gesamten OnBoarding Prozess zusammengehalten wird.</documentation>
    <casePlanModel id="onecaseplanmodel1" name="Case plan model" flowable:formKey="prospectName" flowable:formFieldValidation="false" autoComplete="true">
      <extensionElements>
        <flowable:work-form-field-validation><![CDATA[false]]></flowable:work-form-field-validation>
        <flowable:workformkey><![CDATA[prospectName]]></flowable:workformkey>
        <flowable:static-work-form-key><![CDATA[prospectName]]></flowable:static-work-form-key>
        <design:stencilid><![CDATA[CasePlanModel]]></design:stencilid>
      </extensionElements>
      <planItem id="planItem3" name="OnBoarding initialize" definitionRef="expandedStage1">
        <itemControl>
          <extensionElements>
            <flowable:completionNeutralRule></flowable:completionNeutralRule>
          </extensionElements>
        </itemControl>
      </planItem>
      <planItem id="planItem6" name="OnBoarding enhance" definitionRef="expandedStage2">
        <entryCriterion id="entryCriterion4" sentryRef="sentry3"></entryCriterion>
      </planItem>
      <planItem id="planItem7" name="OnBoarding approve" definitionRef="expandedStage3">
        <entryCriterion id="entryCriterion3" sentryRef="sentry2"></entryCriterion>
      </planItem>
      <sentry id="sentry1">
        <extensionElements>
          <design:stencilid><![CDATA[ExitCriterion]]></design:stencilid>
        </extensionElements>
        <planItemOnPart id="sentryOnPart1" sourceRef="planItem2">
          <standardEvent>occur</standardEvent>
        </planItemOnPart>
      </sentry>
      <sentry id="sentry3">
        <extensionElements>
          <flowable:condition group="true" match="all" targetAttribute="condition">
            <flowable:condition sourceVariableType="outcome" sourceName="prospect" sourceType="string" operator="vars:exists"></flowable:condition>
          </flowable:condition>
          <design:stencilid><![CDATA[EntryCriterion]]></design:stencilid>
        </extensionElements>
        <planItemOnPart id="sentryOnPart2" sourceRef="planItem3">
          <standardEvent>complete</standardEvent>
        </planItemOnPart>
        <ifPart>
          <condition><![CDATA[${vars:exists(prospect)}]]></condition>
        </ifPart>
      </sentry>
      <sentry id="sentry2">
        <extensionElements>
          <design:stencilid><![CDATA[EntryCriterion]]></design:stencilid>
        </extensionElements>
      </sentry>
      <stage id="expandedStage1" name="OnBoarding initialize" autoComplete="true" flowable:includeInStageOverview="true">
        <extensionElements>
          <design:stencilid><![CDATA[ExpandedStage]]></design:stencilid>
          <design:completionneutral><![CDATA[true]]></design:completionneutral>
        </extensionElements>
        <planItem id="planItem1" name="Find prospect" definitionRef="processTask2"></planItem>
        <planItem id="planItem2" name="Cancel OnBoarding" definitionRef="userEventListener1">
          <itemControl>
            <extensionElements>
              <flowable:completionNeutralRule></flowable:completionNeutralRule>
            </extensionElements>
          </itemControl>
        </planItem>
        <processTask id="processTask2" name="Find prospect" flowable:fallbackToDefaultTenant="true" flowable:sameDeployment="true">
          <extensionElements>
            <design:stencilid><![CDATA[ProcessTask]]></design:stencilid>
            <design:stencilsuperid><![CDATA[Task]]></design:stencilsuperid>
            <flowable:in source="prospectName" target="prospectName"></flowable:in>
            <flowable:in source="initiator" target="initiator"></flowable:in>
            <flowable:out source="prospect" target="prospect"></flowable:out>
          </extensionElements>
          <processRefExpression><![CDATA[prospectApplication]]></processRefExpression>
        </processTask>
        <userEventListener id="userEventListener1" name="Cancel OnBoarding">
          <extensionElements>
            <design:stencilid><![CDATA[UserEventListener]]></design:stencilid>
            <design:stencilsuperid><![CDATA[EventListener]]></design:stencilsuperid>
            <design:completionneutral><![CDATA[true]]></design:completionneutral>
          </extensionElements>
        </userEventListener>
      </stage>
      <stage id="expandedStage2" name="OnBoarding enhance" autoComplete="true" flowable:includeInStageOverview="true">
        <extensionElements>
          <design:stencilid><![CDATA[ExpandedStage]]></design:stencilid>
        </extensionElements>
        <planItem id="planItem4" name="Enhance with SwiftKey" definitionRef="enhanceWithSwiftKey">
          <itemControl>
            <extensionElements>
              <flowable:completionNeutralRule></flowable:completionNeutralRule>
            </extensionElements>
            <requiredRule></requiredRule>
          </itemControl>
        </planItem>
        <planItem id="planItem5" name="Enhance with Identifikation Documents" definitionRef="enhanceWithIdentifikationDokuments"></planItem>
        <humanTask id="enhanceWithSwiftKey" name="Enhance with SwiftKey" flowable:assignee="${initiator}" flowable:formKey="swiftKeyForm" flowable:formFieldValidation="false">
          <extensionElements>
            <flowable:task-candidates-type><![CDATA[all]]></flowable:task-candidates-type>
            <design:stencilid><![CDATA[HumanTask]]></design:stencilid>
            <design:stencilsuperid><![CDATA[Task]]></design:stencilsuperid>
            <design:completionneutral><![CDATA[true]]></design:completionneutral>
          </extensionElements>
        </humanTask>
        <humanTask id="enhanceWithIdentifikationDokuments" name="Enhance with Identifikation Documents" flowable:assignee="${initiator}" flowable:formFieldValidation="false">
          <extensionElements>
            <flowable:task-candidates-type><![CDATA[all]]></flowable:task-candidates-type>
            <design:stencilid><![CDATA[HumanTask]]></design:stencilid>
            <design:stencilsuperid><![CDATA[Task]]></design:stencilsuperid>
          </extensionElements>
        </humanTask>
      </stage>
      <stage id="expandedStage3" name="OnBoarding approve" autoComplete="true" flowable:includeInStageOverview="true">
        <extensionElements>
          <design:stencilid><![CDATA[ExpandedStage]]></design:stencilid>
        </extensionElements>
      </stage>
      <exitCriterion id="exitCriterion1" sentryRef="sentry1"></exitCriterion>
    </casePlanModel>
